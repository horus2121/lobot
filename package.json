{
  "name": "pana",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "tauri": "tauri",
    "format": "prettier --write ./src/**/*.{svelte,ts}",
    "lint": "eslint './src/**/*.{ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,svelte}'",
    "precommit": "npm run format && npm run lint:fix",
    "version": "npx auto-changelog -p --starting-version v0.1.0 --ignore-commit-pattern '^(update|doc|chore|(?!.*:)).*'"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.22.1",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tauri-apps/api": "^1.4.0",
    "@tauri-apps/cli": "^1.4.0",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "eslint": "^8.44.0",
    "eslint-plugin-svelte": "^2.32.2",
    "lucide-svelte": "^0.264.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "radix-icons-svelte": "^1.2.1",
    "radix-svelte": "^0.7.1",
    "svelte": "^3.59.2",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.0.0",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "tslib": "^2.6.0",
    "typescript": "^5.1.3",
    "vite": "^4.4.2"
  }
}
